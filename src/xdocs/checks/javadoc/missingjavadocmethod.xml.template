        <macro name="example">
          <param name="path"
                 value="resources/com/puppycrawl/tools/checkstyle/checks/javadoc/missingjavadocmethod/Example6.txt"/>
          <param name="type" value="config"/>
        </macro>
        <p id="Example6-code">Example:</p>
