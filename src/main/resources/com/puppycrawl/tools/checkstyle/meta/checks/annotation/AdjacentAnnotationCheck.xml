<?xml version="1.0" encoding="UTF-8"?>
<checkstyle-metadata>
   <module>
      <check fully-qualified-name="com.puppycrawl.tools.checkstyle.checks.annotation.AdjacentAnnotationCheck"
             name="AdjacentAnnotation"
             parent="com.puppycrawl.tools.checkstyle.TreeWalker">
         <description>Ensures that annotations have no blank lines between their sibling annotations and targets.</description>
         <properties>
            <property default-value="CLASS_DEF,INTERFACE_DEF,ENUM_DEF,METHOD_DEF,CTOR_DEF,VARIABLE_DEF,RECORD_DEF,COMPACT_CTOR_DEF"
                      name="tokens"
                      type="java.lang.String[]"
                      validation-type="tokenSet">
               <description>tokens to check</description>
            </property>
         </properties>
         <message-keys>
            <message-key key="annotation.not.adjacent"/>
         </message-keys>
      </check>
   </module>
</checkstyle-metadata>
