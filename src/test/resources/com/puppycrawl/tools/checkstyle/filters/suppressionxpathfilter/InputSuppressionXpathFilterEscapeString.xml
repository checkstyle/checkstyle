<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suppressions PUBLIC
        "-//Checkstyle//DTD SuppressionXpathFilter Experimental Configuration 1.2//EN"
        "https://checkstyle.org/dtds/suppressions_1_2_xpath_experimental.dtd">
<suppressions>
    <suppress-xpath
            files="InputSuppressionXpathFilterEscapeString.java"
            checks="AvoidEscapedUnicodeCharactersCheck"
            query="/COMPILATION_UNIT/CLASS_DEF[./IDENT
            [@text='InputSuppressionXpathFilterEscapeString']]/OBJBLOCK/
            VARIABLE_DEF[./IDENT[@text='escapedString1']]/ASSIGN/EXPR/
            STRING_LITERAL[@text='\u03bcs']"/>
    <suppress-xpath
            files="InputSuppressionXpathFilterEscapeString.java"
            checks="AvoidEscapedUnicodeCharactersCheck"
            query="/COMPILATION_UNIT/CLASS_DEF[./IDENT
            [@text='InputSuppressionXpathFilterEscapeString']]/OBJBLOCK/
            VARIABLE_DEF[./IDENT[@text='escapedString3']]/ASSIGN/EXPR/
            STRING_LITERAL[@text='\u041a']"/>
</suppressions>
