<?xml version="1.0"?>
<!DOCTYPE import-control PUBLIC
    "-//Checkstyle//DTD ImportControl Configuration 1.5//EN"
    "https://checkstyle.org/dtds/import_control_1_5.dtd">

<import-control package="org.checkstyle.suppressionxpathfilter">
  <forbid package="java.util"/>
  <subpackage name="importcontrol">
    <allow class="java.util.Arrays"/>
    <allow package="java.io"/>
  </subpackage>
</import-control>
